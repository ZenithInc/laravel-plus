{
    "name": "zenithsu/laravel-plus",
    "description": "Laravel-Plus is an efficient Laravel extension package designed to enhance development productivity. It integrates powerful annotation handling and practical helper classes, making the development of Laravel applications more convenient and flexible.",
    "type": "library",
    "require": {
        "php": "^8.2",
        "laravel/framework": "^8.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Zenith\\LaravelPlus\\": "src/"
        }
    },
    "authors": [
        {
            "name": "ZenithSu",
            "email": "happy@hacking.icu"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Zenith\\LaravelPlus\\Providers\\ComponentScanProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "require-dev": {
        "pestphp/pest": "3.x-dev"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
